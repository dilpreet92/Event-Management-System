<%= form_tag('authenticate', method: :get) do %>
  <p>
    <%= label_tag :username, 'UserName' %>
    <%= text_field_tag :username, params[:username]  %>
  </p>
  
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password, params[:password] %>
  </p>
  
  <p>
    <%= submit_tag 'Login' %>
  </p>
<% end %>       
